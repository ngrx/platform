{
  "npmScope": "ngrx",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "workspaceLayout": {
    "appsDir": "projects",
    "libsDir": "modules"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "build-release", "test", "lint", "e2e"]
      }
    }
  },
  "projects": {
    "example-app": {
      "tags": []
    },
    "store": {
      "tags": []
    },
    "effects": {
      "tags": []
    },
    "data": {
      "tags": []
    },
    "entity": {
      "tags": []
    },
    "store-devtools": {
      "tags": []
    },
    "router-store": {
      "tags": []
    },
    "schematics": {
      "tags": []
    },
    "component": {
      "tags": []
    },
    "component-store": {
      "tags": []
    },
    "example-app-e2e": {
      "tags": [],
      "implicitDependencies": ["example-app"]
    },
    "docs-app": {
      "tags": [],
      "implicitDependencies": [
        "store",
        "effects",
        "router-store",
        "store-devtools",
        "entity",
        "data",
        "schematics",
        "component",
        "component-store"
      ]
    }
  }
}
