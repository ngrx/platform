{
  "name": "@ngrx/platform",
  "version": "20.1.0",
  "description": "monorepo for ngrx development",
  "scripts": {
    "ng": "ng",
    "build": "nx run-many --target=build --all",
    "deploy:builds": "npx tsx ./build/deploy-build.ts",
    "test": "nx run-many --target=test --all",
    "clean": "git clean -xdf",
    "coverage:html": "nyc report --reporter=html",
    "copy:dist": "ncp dist/ ./node_modules/@ngrx/",
    "ci": "pnpm exec run test && nyc report --reporter=text-lcov | coveralls",
    "prettier": "prettier --write \"**/*.ts\"",
    "postinstall": "opencollective postinstall",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "copy:schematics": "npx tsx ./build/copy-schematics-core.ts",
    "schematics:check": "git diff --name-only --exit-code ./modules",
    "build:stackblitz": "npx tsx ./build/stackblitz.ts && git add ./stackblitz.html",
    "publish:next": "npx tsx ./build/publish-rc.1",
    "publish:latest": "npx tsx ./build/publish-latest.ts",
    "nx": "nx",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "update:versions": "npx tsx ./build/update-version-numbers.ts",
    "postupdate:versions": "npm run changelog",
    "lint": "nx run-many --target=lint --all",
    "eslint-plugin:update": "npx tsx ./build/generate-eslint-plugin.ts",
    "prepare": "husky install"
  },
  "engines": {
    "node": "^18.19.1 || ^20.11.1 || >=22.0.0",
    "pnpm": "^9.0.0"
  },
  "packageManager": "pnpm@9.10.0",
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "ngrx",
    "angular",
    "rxjs"
  ],
  "author": "Rob Wormald <robwormald@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngrx/platform.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec",
      "**/spec/**/*",
      "/modules/schematics/src/*/files/**/*",
      "/modules/**/schematics/**/files/**/*",
      "**/schematics/src/utility/*"
    ],
    "include": [
      "**/*.ts"
    ]
  },
  "dependencies": {
    "@analogjs/content": "2.1.1",
    "@analogjs/router": "2.1.1",
    "@angular/animations": "21.0.1",
    "@angular/cdk": "21.0.1",
    "@angular/common": "21.0.1",
    "@angular/compiler": "21.0.1",
    "@angular/core": "21.0.1",
    "@angular/elements": "21.0.1",
    "@angular/forms": "21.0.1",
    "@angular/material": "21.0.1",
    "@angular/platform-browser": "21.0.1",
    "@angular/platform-browser-dynamic": "21.0.1",
    "@angular/platform-server": "21.0.1",
    "@angular/router": "21.0.1",
    "@microsoft/api-extractor": "^7.55.1",
    "@microsoft/tsdoc": "^0.15.1",
    "@nx/angular": "22.0.3",
    "firebase-admin": "^12.1.1",
    "firebase-functions": "^5.0.1",
    "front-matter": "^4.0.2",
    "highlight.js": "^11.9.0",
    "marked": "^15.0.7",
    "marked-gfm-heading-id": "^4.1.1",
    "marked-highlight": "^2.2.1",
    "marked-mangle": "^1.1.10",
    "marked-shiki": "^1.2.0",
    "opencollective": "^1.0.3",
    "rxjs": "7.8.0",
    "semver": "^7.7.2",
    "shiki": "~1.6.1",
    "strip-json-comments": "5.0.3",
    "tslib": "^2.7.0",
    "tsx": "^4.11.2",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "2.1.1",
    "@analogjs/vite-plugin-angular": "2.1.1",
    "@analogjs/vitest-angular": "2.1.1",
    "@angular-devkit/build-angular": "21.0.1",
    "@angular-devkit/core": "21.0.1",
    "@angular-devkit/schematics": "21.0.1",
    "@angular-eslint/builder": "21.0.1",
    "@angular-eslint/eslint-plugin": "21.0.1",
    "@angular-eslint/eslint-plugin-template": "21.0.1",
    "@angular-eslint/schematics": "21.0.1",
    "@angular-eslint/template-parser": "21.0.1",
    "@angular-eslint/test-utils": "21.0.1",
    "@angular/build": "21.0.1",
    "@angular/cli": "21.0.1",
    "@angular/compiler-cli": "21.0.1",
    "@angular/language-service": "21.0.1",
    "@nx/cypress": "22.0.3",
    "@nx/eslint": "22.0.3",
    "@nx/eslint-plugin": "22.0.3",
    "@nx/jest": "22.0.3",
    "@nx/node": "22.0.3",
    "@nx/vite": "22.0.3",
    "@nx/workspace": "22.0.3",
    "@octokit/rest": "^15.17.0",
    "@schematics/angular": "21.0.1",
    "@stackblitz/sdk": "^1.11.0",
    "@testing-library/cypress": "9.0.0",
    "@types/jasmine": "4.0.3",
    "@types/jasminewd2": "^2.0.2",
    "@types/jest": "29.5.13",
    "@types/ncp": "^2.0.8",
    "@types/node": "^24.3.0",
    "@types/semver": "^7.7.0",
    "@types/shelljs": "^0.8.17",
    "@typescript-eslint/rule-tester": "8.46.4",
    "@typescript-eslint/types": "8.46.4",
    "@typescript-eslint/utils": "8.46.4",
    "conventional-changelog-cli": "^1.3.21",
    "cpy-cli": "^6.0.0",
    "cypress": "14.2.1",
    "deep-freeze": "^0.0.1",
    "eslint": "9.34.0",
    "eslint-plugin-import": "2.32.0",
    "express": "4.21.2",
    "husky": "^8.0.1",
    "jasmine": "^3.4.0",
    "jasmine-core": "4.2.0",
    "jasmine-marbles": "0.9.2",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-jasmine2": "29.7.0",
    "jest-preset-angular": "14.4.2",
    "jiti": "2.4.2",
    "jsdom": "^22.0.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "^8.0.0",
    "mkdirp": "^3.0.1",
    "ncp": "^2.0.0",
    "ng-packagr": "21.0.0",
    "nx": "22.0.3",
    "ora": "^8.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "~2.59.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rxjs-marbles": "^7.0.1",
    "tinyglobby": "^0.2.14",
    "ts-jest": "29.1.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-snippet": "5.0.2",
    "tsconfig-paths": "^3.1.3",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.0",
    "yaml": "^2.4.5"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/ngrx",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
