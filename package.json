{
  "name": "@ngrx/platform",
  "version": "18.0.2",
  "description": "monorepo for ngrx development",
  "scripts": {
    "ng": "ng",
    "build": "nx run-many --target=build --all",
    "deploy:builds": "ts-node ./build/deploy-build.ts",
    "test": "nx run-many --target=test --all",
    "clean": "git clean -xdf",
    "coverage:html": "nyc report --reporter=html",
    "copy:dist": "ncp dist/ ./node_modules/@ngrx/",
    "ci": "yarn run test && nyc report --reporter=text-lcov | coveralls",
    "prettier": "prettier --write \"**/*.ts\"",
    "postinstall": "opencollective postinstall",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "copy:schematics": "ts-node ./build/copy-schematics-core.ts",
    "schematics:check": "git diff --name-only --exit-code ./modules",
    "build:stackblitz": "ts-node ./build/stackblitz.ts && git add ./stackblitz.html",
    "publish:next": "ts-node ./build/publish-next.ts",
    "publish:latest": "ts-node ./build/publish-latest.ts",
    "nx": "nx",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "update:versions": "ts-node ./build/update-version-numbers.ts",
    "postupdate:versions": "npm run changelog",
    "lint": "nx workspace-lint && ng lint",
    "eslint-plugin:update": "ts-node ./build/generate-eslint-plugin.ts",
    "prepare": "husky install"
  },
  "engines": {
    "node": "^18.19.1 || ^20.11.1 || >=22.0.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "ngrx",
    "angular",
    "rxjs"
  ],
  "author": "Rob Wormald <robwormald@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngrx/platform.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec",
      "**/spec/**/*",
      "/modules/schematics/src/*/files/**/*",
      "/modules/**/schematics/**/files/**/*",
      "**/schematics/src/utility/*"
    ],
    "include": [
      "**/*.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "18.0.0",
    "@angular/cdk": "18.0.0",
    "@angular/common": "18.0.0",
    "@angular/compiler": "18.0.0",
    "@angular/core": "18.0.0",
    "@angular/forms": "18.0.0",
    "@angular/material": "18.0.0",
    "@angular/platform-browser": "18.0.0",
    "@angular/platform-browser-dynamic": "18.0.0",
    "@angular/platform-server": "18.0.0",
    "@angular/router": "18.0.0",
    "@nx/angular": "19.1.1",
    "ajv-formats": "^2.1.1",
    "core-js": "^2.5.4",
    "opencollective": "^1.0.3",
    "rxjs": "7.8.0",
    "semver": "^7.3.6",
    "strip-json-comments": "3.1.1",
    "tslib": "^2.3.1",
    "zone.js": "0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.0.1",
    "@angular-devkit/core": "18.0.1",
    "@angular-devkit/schematics": "18.0.1",
    "@angular-eslint/builder": "18.0.1",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/schematics": "18.0.1",
    "@angular-eslint/template-parser": "18.0.1",
    "@angular-eslint/test-utils": "18.0.1",
    "@angular/cli": "18.0.1",
    "@angular/compiler-cli": "18.0.0",
    "@angular/language-service": "18.0.0",
    "@nx/cypress": "19.1.1",
    "@nx/eslint": "19.1.1",
    "@nx/eslint-plugin": "19.1.1",
    "@nx/jest": "19.1.1",
    "@nx/node": "19.1.1",
    "@nx/workspace": "19.1.1",
    "@octokit/rest": "^15.17.0",
    "@schematics/angular": "18.0.1",
    "@testing-library/cypress": "9.0.0",
    "@types/eslint": "^8.56.10",
    "@types/fs-extra": "^2.1.0",
    "@types/glob": "^5.0.33",
    "@types/globby": "^8.0.0",
    "@types/jasmine": "4.0.3",
    "@types/jasminewd2": "^2.0.2",
    "@types/jest": "29.5.0",
    "@types/lodash": "^4.14.80",
    "@types/ncp": "^2.0.1",
    "@types/node": "^18.16.9",
    "@types/ora": "^1.3.1",
    "@types/prettier": "^1.19.0",
    "@types/rimraf": "^0.0.28",
    "@types/semver": "^7.3.9",
    "@types/shelljs": "^0.8.5",
    "@typescript-eslint/parser": "8.0.0-alpha.20",
    "@typescript-eslint/rule-tester": "8.0.0-alpha.20",
    "@typescript-eslint/utils": "8.0.0-alpha.20",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^1.2.0",
    "conventional-changelog": "^1.1.4",
    "conventional-changelog-cli": "^1.3.21",
    "coveralls": "^2.13.0",
    "cpy-cli": "^1.0.1",
    "cypress": "13.9.0",
    "deep-freeze": "^0.0.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "2.14.0",
    "eslint-plugin-import": "2.26.0",
    "express": "^4.16.4",
    "fs-extra": "^10.1.0",
    "glob": "^7.1.2",
    "husky": "^8.0.1",
    "jasmine": "^3.4.0",
    "jasmine-core": "4.2.0",
    "jasmine-marbles": "0.9.2",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-jasmine2": "29.5.0",
    "jest-preset-angular": "14.1.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "^8.0.0",
    "ncp": "^2.0.0",
    "ng-packagr": "18.0.0",
    "npm-run-all": "^4.1.5",
    "nx": "19.1.1",
    "nyc": "^10.1.2",
    "ora": "^1.3.0",
    "prettier": "^2.7.1",
    "protractor": "7.0.0",
    "reflect-metadata": "^0.1.9",
    "replace-in-file": "^3.1.1",
    "rimraf": "^2.5.4",
    "rollup": "~2.59.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rxjs-marbles": "^7.0.1",
    "shelljs": "^0.8.3",
    "sorcery": "^0.10.0",
    "ts-jest": "29.1.0",
    "ts-loader": "^5.3.3",
    "ts-node": "10.9.1",
    "ts-snippet": "5.0.2",
    "tsconfig-paths": "^3.1.3",
    "tsickle": "^0.37.0",
    "tsutils": "2.27.2",
    "typescript": "5.4.5",
    "typescript-eslint": "8.0.0-alpha.20",
    "uglify-js": "^3.1.9"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/ngrx",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
  "resolutions": {
    "jasmine-marbles": "0.9.1",
    "lodash": "^4.17.11"
  }
}
